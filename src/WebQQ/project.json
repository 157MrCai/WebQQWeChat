{

  "version": "1.0.0-*",

  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG" ]
      }
    },
    "Release": {
      "buildOptions": {
        "optimize": true
      }
    }
  },

  "buildOptions": {
    "emitEntryPoint": true,
    "copyToOutput": {
      "include": [ "Resources/hash.js" ],
      "exclude": [ "" ],
      "excludeFiles": [ "" ],
      "builtIns": {},
      "mappings": {}
    }
  },

  "dependencies": {
    "Newtonsoft.Json": "9.0.1",
    "Microsoft.Extensions.Logging": "1.0.0",
    "Microsoft.Extensions.Logging.Console": "1.0.0",
    "HttpActionFrame": "1.0.0-*",
    "Utility": "1.0.0-*"
  },

  "frameworks": {
    "net461": {
      "frameworkAssemblies": {
        "System.Drawing": "4.0.0.0",
        "System.Runtime": "4.0.20.0"

      },
      "dependencies": {
        "System.Net.Http": "4.0.0",
        "Jint": "2.9.1"
      },
      "buildOptions": {
        "define": [ "NET" ],
        "platform": "anycpu"
      }
    },
    "netcoreapp1.1": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.1-*",
          "type": "platform"
        },
        "System.Collections.NonGeneric": "4.0.1",
        "System.ComponentModel.Primitives": "4.1.0",
        "Jint-netcore": "2.7.1-final",
        "System.Text.Encoding.CodePages": "4.0.1",
        "CoreCompat.System.Drawing": "1.0.0-*",
        "runtime.linux-x64.CoreCompat.System.Drawing": "1.0.0-*",
        "runtime.osx.10.10-x64.CoreCompat.System.Drawing": "1.0.1-*"
      },
      "buildOptions": {
        "define": [ "NETCORE" ],
        "debugType": "portable"
      }
    }

  }
}
